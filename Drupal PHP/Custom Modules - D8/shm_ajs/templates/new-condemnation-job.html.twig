                        <div class="form-item field-liferaft-vessel" >
                            <label class="control-label">Vessel</label>
                            <input type="text" class="form-date form-control" auto-complete="getMatchedLiferaftVessel" ng-model="liferaftNew.vessel.title">
                        </div>
                        <div class="form-item date-of-manufacturing">
                            <label class="control-label">Manufacturing Date</label>
                            <input type="date" class="form-date form-control" ng-model="liferaftNew.field_liferaft_date_manufacturing">
                        </div>
                        <div class="form-item field-liferaft-operating-mechanism " >
                            <label class="control-label">Operating Mechanism </label>
                            <select class="form-select form-control" data-toggle="tooltip" ng-model="liferaftNew.field_liferaft_operating_mechanism">
                                <option value="key1">Throw Overboard Liferfat</option>
                                <option value="key2">Throw Overboard Self Righting Liferaft</option>
                                <option value="key3">Davit Launched Liferaft</option>
                                <option value="key4">Davit Launched Self Righting Liferaft</option>
                            </select>
                        </div>
                        <div class="form-item field-liferaft-manufacturer" >
                            <label class="control-label">Manufacturer</label>
                            <input type="text" class="form-date form-control" auto-complete="getMatchedLiferaftManufacturer" ng-model="liferaftNew.field_liferaft_manufacturer.title">
                        </div>
                        <div class="form-item field-liferaft-previous-inspec" >
                            <label class="control-label">Last Surveyed On</label>
                            <input type="date" class="form-date form-control" ng-model="liferaftNew.field_liferaft_previous_inspec">
                        </div>
                        <div class="form-item date-of-inspection">
                            <label class="control-label">Surveyed On</label>
                            <input type="date" class="form-date form-control" ng-model="liferaftNew.field_liferaft_date_inspection">
                        </div>
                        <div class="form-item field-liferaft-type" >
                            <label class="control-label">Type</label>
                            <input type="text" class="form-date form-control" ng-model="liferaftNew.field_liferaft_type.type" auto-complete="getMatchedLiferaftType">
                        </div>
                        <div class="form-item field-liferaft-survivalpack-type" >
                            <label class="control-label">Survival Pack Type</label>
                            <select class="form-select form-control" data-toggle="tooltip" ng-model="liferaftNew.field_liferaft_survivalpack_type">
                                <option value="_none">- None -</option>
                                <option value="A-Pack">A-Pack Solas</option>
                                <option value="B-Pack">B-Pack Solas</option>
                                <option value="Partial Pack">Partial Pack Solas</option>
                            </select>
                        </div>
                        <div class="form-item field-liferaft-model" >
                            <label class="control-label">Model</label>
                            <input type="text" class="form-date form-control" ng-model="liferaftNew.field_liferaft_model">
                        </div>


                        <div class="form-item field-liferaft-submit-condemnation">
                            <button class="btn-primary form-submit" value="submit-new-job" type="submit" name="submit-new-job" ng-click="addNewLiferaft()">Save</button>
                        </div>
                             </div>
                </div>
            </form>
        </script>
    </div>
</div>
</div>